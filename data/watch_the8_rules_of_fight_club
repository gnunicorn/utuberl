		<script>
		onLoadFunctionList.push(function() {
			if (!window['player_written'])
				return;
				if (isFlashVersionSupported(9,0,115)) {
					if (canPlayH264Videos()) {
						yt.UserPrefs.set('ftuc', 10);
					}
				} else {
					_gel('flash10-promo-div').style.display = 'block';
					var showCount = yt.UserPrefs.get('ftuc');
					if (!showCount) {
						showCount = 0;
					}
					yt.UserPrefs.set('ftuc', parseInt(showCount) + 1);
					yt.UserPrefs.save();
				}
		});	
		</script>














